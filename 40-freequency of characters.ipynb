   "source": [
    "# 40 frequency of characters\n",
    "string=input(\"Enter the string:\")\n",
    "freq={}\n",
    "for i in string:\n",
    "    if i in freq:\n",
    "        freq[i]=freq[i]+1\n",
    "    else:\n",
    "        freq[i]=1\n",
    "for keys, value in freq.items():\n",
    "    print(keys,\"is repeated\",value,\"times\")"
   ]
